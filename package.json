{
  "name": "email-warmup",
  "version": "3.0.0",
  "description": "Gmail warm-up automation with Next.js, PostgreSQL (Supabase), and Node.js",
  "main": "index.js",
  "packageManager": "pnpm@8.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "warmup": "node services/warmup.js",
    "warmup:dev": "nodemon services/warmup.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "docker:build": "docker build -t gmail-warmup .",
    "docker:run": "docker run -d -p 3000:3000 --env-file .env gmail-warmup",
    "prod:setup": "pnpm install && pnpm db:migrate:deploy && pnpm build",
    "prod:start": "pnpm pm2:start",
    "test": "tsx scripts/test-warmup.ts",
    "setup": "npm run db:generate && npm run db:push",
    "fix:user-ids": "tsx scripts/fix-user-ids.ts"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": [
    "gmail",
    "automation",
    "warmup",
    "email",
    "supabase",
    "postgresql",
    "prisma",
    "nextjs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "@types/formidable": "^3.4.6",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.5",
    "dotenv": "^16.3.1",
    "formidable": "^3.5.4",
    "imap-simple": "^5.1.0",
    "next": "^14.0.4",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/imap-simple": "^4.2.10",
    "@types/node": "^20.19.30",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.16",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "prisma": "^5.8.1",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  },
  "private": true
}
